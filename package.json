{
  "name": "file-content-aggregator",
  "productName": "File Content Aggregator",
  "private": true,
  "version": "1.0.0",
  "description": "Aggregates content from multiple files based on search criteria.",
  "author": {
    "name": "Mosch"
  },
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:profile": "npm-run-all --parallel dev:react dev:electron:profile",
    "dev:electron:profile": "npm run transpile:electron && cross-env NODE_ENV=development electron . --profile",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron . -- ",
    "build": "npm run transpile:electron && vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:mac": "npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:linux": "npm run build && electron-builder --linux --x64",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-fuses disable:runAsNode disable:nodeCliInspect",
    "format": "prettier . --write",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "test:performance": "node scripts/runPerformanceTests.js",
    "test:complexity": "node scripts/analyzeCodeComplexity.js src",
    "test:compare": "node scripts/comparePerformance.js",
    "test:metrics": "node scripts/countLinesOfCode.js",
    "test:basic-performance": "node scripts/runBasicPerformanceTests.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-store": "^10.0.1",
    "fast-glob": "^3.3.3",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.2",
    "jsep": "^1.4.0",
    "lucide-react": "^0.487.0",
    "mime": "^4.0.3",
    "p-limit": "^6.2.0",
    "picomatch": "^4.0.2",
    "react": "^19.0.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/picomatch": "^4.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.1.2",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "commander": "^11.1.0",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6"
  }
}
